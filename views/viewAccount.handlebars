<style>
table {
  font-family: arial, sans-serif;
  width: 60%;
  margin-left: 600px;
  margin-bottom: 30px;
}

h2 {
  margin-left: 950px;
}

h4, li {
  margin-left: 600px;
}


td, th {
  border: 1px solid #dddddd;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}

</style>

<!-- Side navigation -->
<div class="sidenav">
  <a href="account">Create a New Account</a>
</div>


<h2>Account List</h2>

<table>
  <tr>
    <th>E-mail</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Date of Birth</th>
    <th>Phone</th>

  </tr>

  <tr>
    <td>{{email}}</td>
    <td>{{firstName}}</td>
    <td>{{lastName}}</td>
    <td>{{dateOfBirth}}</td>
    <td>{{phone}}</td>
  </tr>

</table>


<button id="search" style="  width: 320px;  height: 45px; border: none; background: black;  color: white;  margin-left: 900px;  margin-bottom: 30px;">Search</button>

<div id="searchButton" style=" margin-left: 900px;"> </div>

<script>
  document.getElementById("search").addEventListener("click", function(button)
  {
    document.getElementById("searchButton").innerHTML = "Will search members from the table"
  });

</script>

